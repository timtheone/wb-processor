{
  "name": "wb-processor-monorepo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start-all-dev": "concurrently --kill-others \"bun run --hot ./packages/backend/src/index.ts\" \"bun run --hot ./packages/bot-client/index.ts\"",
    "start-all": "concurrently --kill-others \"bun ./packages/backend/src/index.ts\" \"bun ./packages/bot-client/index.ts\""
  },
  "dependencies": {
    "concurrently": "^8.2.2"
  }
}